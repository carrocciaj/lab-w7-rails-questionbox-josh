<h2>Log In</h2>
<% if flash[:error_message]%>
<p class="flash-error"><%= flash[:error_message] %></p>
<% end %>
<%= form_with(url: session_path) do %>
   <p>
    <%=label_tag :email %>
    <%=text_field_tag :email %>
   </p> 
   <p>
    <%=label_tag :password %>
    <%=password_field_tag :password %>
   </p>
    <%=submit_tag "Log in", class: "button" %>
<% end %>

<%= link_to "Create an Account", new_user_path %>
